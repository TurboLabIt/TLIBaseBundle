{
    "name": "turbolabit/tli-base-bundle",
    "description": "A collection of reusable Symfony components by TurboLab.it",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Zane",
            "email": "info@turbolab.it"
        }
    ],
    "autoload": {
        "psr-4": {
